{
"manifest_version": 2,
"author": "Triple",
"name": "TweetMoment", 
"description": "Edit tweet in the moment that you are not able to see in it.",
"version": "1.0.0",
"icons": {
"128": "icon.png"
},
"permissions": [
"activeTab",
"https://twitter.com/*"
],
"content_scripts": [{
"matches": ["https://twitter.com/*"],
"js": ["content.js"],
"run_at": "document_end"
}]
}